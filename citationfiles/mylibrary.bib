@article{yeagerUsingDesignThinking2016b,
  title = {Using {{Design Thinking}} to {{Improve Psychological Interventions}}: {{The Case}} of the {{Growth Mindset During}} the {{Transition}} to {{High School}}},
  shorttitle = {Using {{Design Thinking}} to {{Improve Psychological Interventions}}},
  author = {Yeager, David S. and Romero, Carissa and Paunesku, Dave and Hulleman, Christopher S. and Schneider, Barbara and Hinojosa, Cintia and Lee, Hae Yeon and O'Brien, Joseph and Flint, Kate and Roberts, Alice and Trott, Jill and Greene, Daniel and Walton, Gregory M. and Dweck, Carol S.},
  date = {2016-04},
  journaltitle = {Journal of educational psychology},
  shortjournal = {J Educ Psychol},
  volume = {108},
  pages = {374--391},
  issn = {0022-0663},
  doi = {10.1037/edu0000098},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4981081/},
  urldate = {2020-09-09},
  abstract = {There are many promising psychological interventions on the horizon, but there is no clear methodology for preparing them to be scaled up. Drawing on design thinking, the present research formalizes a methodology for redesigning and tailoring initial interventions. We test the methodology using the case of fixed versus growth mindsets during the transition to high school. Qualitative inquiry and rapid, iterative, randomized ``A/B'' experiments were conducted with \textasciitilde 3,000 participants to inform intervention revisions for this population. Next, two experimental evaluations showed that the revised growth mindset intervention was an improvement over previous versions in terms of short-term proxy outcomes (Study 1, N=7,501), and it improved 9th grade core-course GPA and reduced D/F GPAs for lower achieving students when delivered via the Internet under routine conditions with \textasciitilde 95\% of students at 10 schools (Study 2, N=3,676). Although the intervention could still be improved even further, the current research provides a model for how to improve and scale interventions that begin to address pressing educational problems. It also provides insight into how to teach a growth mindset more effectively.},
  eprint = {27524832},
  eprinttype = {pmid},
  file = {/Users/wujiawen/Zotero/storage/9X3CJZ6Y/Yeager et al. - 2016 - Using Design Thinking to Improve Psychological Int.pdf},
  number = {3},
  pmcid = {PMC4981081}
}

